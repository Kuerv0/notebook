  standby 20 ip 192.168.2.67
  standby 20 priority 101
  standby 20 preempt

  ipv6 address fe80::2 link-local
  ipv6 address 3000:0000:0000:4000::1/50

  ipv6 nd managed-config-flag
! ipv6 nd prefix 
  ipv6 dhcp server DEADFUL
  no shutdown 

