  ip address 192.168.2.194 255.255.255.192

  standby 40 ip 192.168.2.195
  standby 40 priority 101
  standby 40 preempt

  ipv6 address fe80::2 link-local
  ipv6 address 3000:0000:0000:c000::1/50
  no shutdown

line vty 0 4
  password omar
  login 
  transport input telnet

! }

